{
  "name": "eslint-config",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:underspare/eslint-config.git",
  "author": "shanejix <shanejix@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run dev:eslintrc && npm run dev:copyfiles && npm run dev:webpack-dev-server",
    "dev:eslintrc": "ts-node scripts/build.ts",
    "dev:copyfiles": "npm run build:copyfiles",
    "dev:webpack-dev-server": "webpack serve --open --config ./website/webpack.config.js",
    "build:copyfiles": "copyfiles -u 2 \"website/public/**/*\" dist/public",
    "build:website": "npm run dev:eslintrc && npm run dev:copyfiles && webpack --config ./website/webpack.config.js"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.1",
    "@types/doctrine": "^0.0.5",
    "@types/eslint": "^7.28.1",
    "@types/node": "^17.0.5",
    "@types/prettier": "^2.4.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/xml-escape": "^1.1.0",
    "cookie": "^0.4.2",
    "copyfiles": "^2.4.1",
    "doctrine": "^3.0.0",
    "html-webpack-plugin": "^5.3.2",
    "insert-tag": "^0.1.2",
    "prettier": "^2.4.1",
    "querystring": "^0.2.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.0",
    "react-syntax-highlighter": "^15.4.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "url": "^0.11.0",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "xml-escape": "^1.1.0"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.8",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1"
  }
}